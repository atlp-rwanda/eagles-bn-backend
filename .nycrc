{
  "reporter": [
    "text",
    "html",
    "lcov",
    "text-summary"
  ],
  "require": [
    "@babel/register"
  ],
  "exclude": ["src/database","coverage","nyc_output","src/tests", "public"],
  "sourceMap": false,
  "instrument": false,
  "all":true,
  "cache": false,
  "check-coverage":true,
 "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [70, 95],
    "statements": [80, 95]
  }
}
