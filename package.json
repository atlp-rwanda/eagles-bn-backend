{
  "name": "eagles-bn-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "migrate": "npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo:all",
    "migrate:test": "npx sequelize db:migrate --env test",
    "droptables:dev": "npx sequelize db:migrate:undo:all",
    "db:seed:test": "cross-env NODE_ENV=test npx sequelize db:seed:all",
    "start": "npm run seed && npm run build && node build",
    "build": "babel ./src --out-dir build",
    "lint": "eslint --fix src",
    "dev": "nodemon --exec babel-node -r dotenv/config  src",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run seed:test && cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --require dotenv/config  src/tests/*.test.js --timeout 25000 --exit || true",
    "testing": "cross-env NODE_ENV=test mocha --require @babel/register --require dotenv/config  src/tests/comment.test.js --timeout 15000 --exit || true",
    "cover": "nyc --reporter=html --reporter=text npm test",
    "seed": "npm run migrate:undo && npm run migrate &&  sequelize db:seed:all",
    "gen:models": " sequelize-auto -o \"./src/database/models\" -d eagles -h localhost -u postgres -p 5432 -x \"Jannyda1\" -e postgres -l es6",
    "seed:test": "npm run migrate:undo -- --env test && npm run migrate:test && npx sequelize db:seed:all --env test"
  },
